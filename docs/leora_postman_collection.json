    { "key": "baseUrl", "value": "http://localhost:9090/api/v1" },
    { "key": "accessToken", "value": "" },
    { "key": "refreshToken", "value": "" },
    { "key": "userId", "value": "" },
    { "key": "taskId", "value": "" },
    { "key": "goalId", "value": "" },
